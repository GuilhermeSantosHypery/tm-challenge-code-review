model Weather {
    id          String    @id @default(uuid()) @db.Uuid()
    cloudPct    Int?      @map("cloud_pct")
    temp        Int?      @map("temp")
    feelsLike   Int?      @map("feels_like")
    humidity    Int?      @map("humidity")
    minTemp     Int?      @map("min_temp")
    maxTemp     Int?      @map("max_temp")
    windSpeed   Float?    @map("wind_speed")
    windDegrees Int?      @map("wind_degrees")
    sunrise     DateTime? @map("sunrise")
    sunset      DateTime? @map("sunset")

    createdAt DateTime @default(now()) @map("created_at")
    updatedAt DateTime @updatedAt @map("updated_at")

    @@map("weather")
}
